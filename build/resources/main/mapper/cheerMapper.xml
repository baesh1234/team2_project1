<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team2_project1.project1.mapper.CheerMapper">
    <select id="getAllCheer" resultType="com.team2_project1.project1.model.Cheer">
        SELECT * FROM cheer_comments
    </select>
    <insert id="insertCheer" parameterType="com.team2_project1.project1.model.Cheer">
        INSERT INTO cheer_comments (comment_content, email)
        VALUES (#{commentContent}, #{email})
    </insert>
</mapper>
